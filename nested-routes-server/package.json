{
  "name": "nested-routes-server",
  "description": "Server for Nested Routes",
  "version": "1.0.0",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:debug": "ndb ts-node src/index.ts",
    "start:prod": "node dist",
    "dev": "nodemon --watch src -e ts --exec yarn start",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write README.md 'src/**/*.ts'"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.3",
    "babel-jest": "^27.4.2",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
