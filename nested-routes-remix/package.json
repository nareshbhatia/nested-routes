{
  "name": "nested-routes-remix",
  "description": "Evaluation of nested routes",
  "version": "0.1.0",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "yarn postcss:run --env production",
    "dev": "concurrently \"yarn postcss:run --watch\" \"remix dev\"",
    "format": "prettier --write README.md 'app/**/{*.md,*.json,*.css,*.ts*}'",
    "postcss:run": "postcss styles/**/*.css --base styles --dir app/styles",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cssnano": "^5.0.12",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
